}

.search-button {
  background: #2563eb; /* Original blue */
  border: none;
  color: white;
  padding: 8px 12px;
  border-radius: 6px;
  cursor: pointer;
  font-size: 0.9rem;
  transition: background-color 0.3s ease;
}

.search-button:hover {
  background-color: #1d4ed8; /* Darker blue on hover */
}

.cart-icon {
  position: relative;
  color: white;
  font-size: 1.4rem;
  cursor: pointer;
  transition: color 0.3s ease;
}
.cart-icon:hover {
  color: #60a5fa; /* Light blue hover */
}

.cart-count {
  position: absolute;
  top: -6px;
  right: -10px;
  background-color: #ef4444;
  color: white;
  font-size: 0.7rem;
  padding: 2px 6px;
  border-radius: 50%;
  font-weight: bold;
}

.container { /* This is a general container, used in body too */
  max-width: 1200px;
  margin: 0 auto;
  padding: 2rem 20px;
}

.hero {
  padding: 40px 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  gap: 2rem;
  background-color: #ffffff; /* White background for hero section */
  border-radius: 12px;      /* Rounded corners for hero section */
  padding: 3rem;            /* More padding within hero */
  margin-bottom: 2rem;      /* Space below hero */
}

.hero-text {
  max-width: 500px;
}

.hero h1 {
  font-size: 2.5rem; /* Larger H1 */
  margin-bottom: 1rem; /* Increased margin */
  color: #111827; /* Darker heading color */
}

.hero p {
  color: #4b5563; /* Slightly darker for better contrast */
  margin: 1rem 0 2rem; /* Adjusted margins */
  font-size: 1.1rem; /* Slightly larger */
}

.hero img {
  max-width: 350px; /* Slightly larger image */
  width: 100%;
  border-radius: 12px;
}

.btn {
  background: #1d4ed8; /* Darker blue for better contrast */
  color: white;
  padding: 14px 28px; /* Slightly more padding */
  text-decoration: none;
  border-radius: 8px;
  display: inline-block;
  font-weight: bold;
  transition: background-color 0.3s ease, transform 0.2s ease; /* Added transition */
  border: none; /* Ensure no default border */
  cursor: pointer;
}

.btn:hover {
  background-color: #1e40af; /* Darker shade on hover */
  transform: scale(1.03);    /* Slight scale effect */
}

h2,
h3 { /* Section titles */
  margin-top: 3rem;
  font-size: 2rem; /* Slightly larger section titles */
  margin-bottom: 1.5rem; /* Increased margin */
  text-align: center;
  color: #111827; /* Darker heading color */
}

.products {
  display: grid; /* Using grid for better alignment */
  grid-template-columns: repeat(auto-fit, minmax(220px, 1fr)); /* Responsive grid */
  gap: 1.5rem; /* Increased gap */
  justify-content: center;
}

.card {
  border: 1px solid #e5e7eb;
  border-radius: 12px;
  padding: 1.5rem;
  text-align: center;
  background: #ffffff; /* White background for cards */
  box-shadow: 0 4px 6px rgba(0,0,0,0.05); /* Softer initial shadow */
  transition: box-shadow 0.3s ease, transform 0.3s ease;
  display: flex; /* For better internal alignment */
  flex-direction: column; /* Align items vertically */
  justify-content: space-between; /* Distribute space */
}

.card:hover {
  box-shadow: 0 10px 15px rgba(0,0,0,0.1); /* More prominent shadow on hover */
  transform: translateY(-5px);             /* Lift effect */
}

.card img {
  width: 100%; /* Make image responsive within card */
  max-width: 150px; /* Max width for product image */
  height: auto;
  margin-bottom: 1rem;
  align-self: center; /* Center image */
}

.card h4 {
  font-size: 1.1rem;
  margin-bottom: 0.5rem;
  color: #1f2937;
}

.card p { /* Price styling */
  font-size: 1.25rem; /* Slightly larger price */
  font-weight: 600; /* Semi-bold */
  color: #1d4ed8;
  margin-bottom: 1rem;
}

.card .btn {
    margin-top: auto; 
}


.categorias {
  margin-top: 3rem; /* Increased margin */
}

.categorias-lista {
  display: flex;
  gap: 1rem; /* Adjusted gap */
  flex-wrap: wrap;
  justify-content: center;
}

.categorias-lista button {
  padding: 12px 24px; /* Increased padding */
  border: 1px solid #d1d5db; /* Added border */
  background: #ffffff; /* White background */
  border-radius: 8px;
  font-weight: 600; /* Semi-bold */
  cursor: pointer;
  transition: background-color 0.3s ease, color 0.3s ease, border-color 0.3s ease;
  color: #374151; /* Default text color */
}

.categorias-lista button:hover {
  background: #eef2ff; /* Light blue background on hover */
  color: #1d4ed8;      /* Blue text on hover */
  border-color: #1d4ed8; /* Blue border on hover */
}

.info-banner {
  background: #ffffff; /* White background */
  display: flex;
  justify-content: space-around;
  padding: 1.5rem 0; /* Increased padding */
  margin: 3rem 0; /* Adjusted margin */
  font-weight: 600; /* Semi-bold */
  font-size: 1rem;
  border-radius: 12px; /* Rounded corners */
  box-shadow: 0 4px 6px rgba(0,0,0,0.05); /* Subtle shadow */
}

.info-banner div {
    color: #374151; /* Text color */
}

.venda-banner {
  background: #e0f2fe; /* Original light blue */
  padding: 2rem; /* Increased padding */
  text-align: center;
  border-radius: 12px;
  margin-top: 3rem; /* Adjusted margin */
}

.venda-banner p {
    font-size: 1.1rem; /* Slightly larger text */
