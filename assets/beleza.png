}

.venda-banner .btn {
  margin-top: 1rem;
}

.footer {
  background: #111827; /* Darker footer background */
  color: #d1d5db;    /* Lighter text color for footer */
  padding: 3rem 2rem; /* Increased padding */
  margin-top: 4rem;
  text-align: center;
}

.footer-grid {
  display: grid; /* Using grid for footer layout */
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 2rem;
  text-align: left;
  max-width: 1200px; /* Consistent max-width */
  margin: 0 auto 2rem auto; /* Center grid */
}

.footer h4 {
  margin-bottom: 1rem; /* Increased margin */
  font-size: 1.1rem; /* Slightly larger footer headings */
  color: #ffffff; /* White headings */
}

.footer ul {
  list-style-type: none; /* Removed disc */
  padding-left: 0; /* Removed padding for no-disc */
  line-height: 1.8;
}

.footer ul li a {
  color: #9ca3af;
  text-decoration: none;
  font-size: 0.95rem;
  transition: color 0.3s ease;
}

.footer ul li a:hover {
  color: #e5e7eb;
}

.footer hr {
  border-color: #374151; /* Darker hr */
  margin: 2rem 0 1rem;
}

.footer .footer-bottom-text { 
    font-size: 0.9rem;
    color: #9ca3af;
}

.carousel {
  position: relative;
  max-width: 100%;
  margin: 2rem auto; /* Centered carousel */
  overflow: hidden;
  border-radius: 12px; /* Rounded corners for carousel */
  box-shadow: 0 6px 12px rgba(0,0,0,0.1); /* Added shadow */
}

.carousel-container {
  display: flex;
  transition: transform 0.7s ease-in-out; /* Smoother transition */
}

.slide {
  min-width: 100%;
  position: relative;
  display: none; 
}

.slide.active {
  display: block; 
}

.slide img {
  width: 100%;
  display: block;
  max-height: 400px; 
  object-fit: cover; 
}

.slide-text {
  position: absolute;
  bottom: 15%; 
  left: 5%;    
  color: white;
  background-color: rgba(0,0,0,0.6); 
  padding: 1.5rem 2.5rem; 
  border-radius: 10px;
  max-width: 80%; 
}

.slide-text h3 {
  margin-top: 0; 
  margin-bottom: 0.75rem; 
  font-size: 1.8rem; 
  color: #ffffff; 
}

.slide-text .btn { 
    padding: 10px 20px;
    font-size: 0.9rem;
}

.carousel button.prev,
.carousel button.next {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background-color: rgba(0,0,0,0.4); 
  color: white;
  border: none;
  padding: 0.8rem; 
  cursor: pointer;
  border-radius: 50%;
  font-size: 1.8rem;
  transition: background-color 0.3s ease, transform 0.2s ease;
  width: 40px; 
  height: 40px; 
  display: flex;
  align-items: center;
  justify-content: center;
}

.carousel button.prev:hover,
.carousel button.next:hover {
  background-color: rgba(0,0,0,0.7);
  transform: translateY(-50%) scale(1.1); 
}

.carousel button.prev {
  left: 15px; 
}

.carousel button.next {
  right: 15px; 
}

@media (max-width: 768px) {
  header.topo {
    position: static; 
  }
  header .container {
    flex-direction: column;
    align-items: center; 
    gap: 15px;
  }
  nav {
      justify-content: center; 
